/// Wraps the linked list used by cUrl
class sList {
    slist head, tail;
    oper init(slist base) : head = base, tail=head {}
    oper init(Buffer d) : head = slist(d.buffer), tail=head {}

    void append(Buffer d) {
        tail = tail.append(d.buffer);
    }

    ## A linked list iterator.
    class Iter {
        slist _node;
        bool __first;
        oper init(slist node) : _node = node, __first = true {}

        byteptr elem() {
                return _node.data;
        }

        bool next() {
            if (_node is null)
                return false;

            _node = _node.next;
            return !(_node is null);
        }

        bool nx() {
            if (_node is null)
                return false;

            if (__first)
                __first = false;
            else
                _node = _node.next;
            return !(_node is null);
        }

        bool isTrue() { return !(_node is null); }
    }

    Iter iter() {
        return Iter(head);
    }
}

class CookieMaker[M] {
    @static
    int set(CURL handle, M cmap){
        AppendBuffer cstring = {256};
        uint i=0;
        for (elem :in cmap) {
            if (i>0) cstring.extend(" ");
            cstring.extend(elem.key);
            cstring.extend("=");
            cstring.extend(elem.val);
            cstring.extend(";");
            i+=1;
        }
        cstring.extend("\0");
        return easy_setopt_ptr(handle, CURLOPT_COOKIE, cstring.buffer);
    }
}

class CurlEasy {
    CURL _handle;
    sList _headers = null;
    int _lastResult = 0;
    OrderedHashMap[String, String] _cookies = {};
    CURLinfo _info = {0};

    oper init() : _handle = easy_init() { }
    oper init(String url) : _handle = easy_init() {
      easy_setopt_ptr(_handle, CURLOPT_URL, url.buffer);
    }

    CURL getHandle() { return _handle; }

    int perform() {
        _lastResult = easy_perform(_handle);
        return _lastResult;
    }

    CString strError() {
        return CString(easy_strerror(_lastResult), false);
    }

    CString strError(int result) {
        return CString(easy_strerror(result), false);
    }

    int pause(int bitmask) {
        return easy_pause(_handle, bitmask);
    }

    int setOpt(int optName, Buffer data) {
        return easy_setopt_ptr(_handle, optName, data.buffer);
    }

    int setOpt(int optName, sList list) {
        return easy_setopt_ptr(_handle, optName, list.head);
    }

    int setOpt(int optName, voidptr f) {
        return easy_setopt_ptr(_handle, optName, f);
    }

    int setOpt(int optName, int value) {
        return easy_setopt_long(_handle, optName, int64(value));
    }

    int64 getInfoLong(int infoName) {
        _info.info = infoName;
        result := easy_info_get_long(_handle, _info);
        if (result != CURLE_OK)
            throw Exception(CString(easy_strerror(result), false));
        return _info.resultLong;
    }

    float64 getInfoDouble(int infoName) {
        _info.info = infoName;
        result := easy_info_get_double(_handle, _info);
        if (result != CURLE_OK)
            throw Exception(CString(easy_strerror(result), false));
        return _info.resultDouble;
    }

    CString getInfoString(int infoName) {
        _info.info = infoName;
        result := easy_info_get_ptr(_handle, _info);
        if (result != CURLE_OK)
            throw Exception(CString(easy_strerror(result), false));
        return CString(byteptr(_info.resultPtr), false);
    }

    slist getInfoSList(int infoName) {
        _info.info = infoName;
        result := easy_info_get_slist(_handle, _info);
        if (result != CURLE_OK)
            throw Exception(CString(easy_strerror(result), false));
        return slist_cast(_info.resultPtr);
    }

    int addCookie(String key, String value) {
        _cookies[key] = value;
        return _cookies.count();
    }

    int saveCookies() {
        return CookieMaker[OrderedHashMap[String, String]].set(_handle, _cookies);
    }

    int setCookies(OrderedHashMap[String, String] map) {
        _cookies = map;
        return saveCookies();
    }

    int setCookies(HashMap[String, String] map) {
        for (e :in map)
            _cookies[e.key] = e.val;
        return saveCookies();
    }

    void addHeader(Buffer headerName, Buffer headerValue) {
        headerString := FStr() `$headerName: $headerValue`;
        if (_headers is null)
            _headers = sList(headerString);
        _headers.append(headerString);
    }

    int setHeader(Buffer headerName, Buffer headerValue) {
        addHeader(headerName, headerValue);
        return easy_setopt_ptr(_handle, CURLOPT_HTTPHEADER, _headers.head);
    }

    int setHeaders(HashMap[String, String] map) {
        for (e :in map) addHeader(e.key, e.val);
        return easy_setopt_ptr(_handle, CURLOPT_HTTPHEADER, _headers.head);
    }

    int setHeaders(OrderedHashMap[String, String] map) {
        for (e :in map) addHeader(e.key, e.val);
        return easy_setopt_ptr(_handle, CURLOPT_HTTPHEADER, _headers.head);
    }

    void clearHeaders() {
        _headers = null;
    }

    int saveHeaders() {
        if (!(_headers is null))
            return easy_setopt_ptr(_handle, CURLOPT_HTTPHEADER, _headers.head);
        else
            return CURLE_OK;
    }

    oper del() {
        if (!(_handle is null)) easy_cleanup(_handle);
    }
}

String urlEncode(String plain) {
    temp := curl_escape(plain.buffer, plain.size);
    result := String(temp);
    curl_free(temp);
    return result;
}

String urlDecode(String plain) {
    temp := curl_unescape(plain.buffer, plain.size);
    result := String(temp);
    curl_free(temp);
    return result;
}

class FormData {
    OrderedHashMap[String, String] _data = {};
    String _raw;
    bool _current;

    oper init() : _current = true, _raw = "" { }

    uint _parse() {
        items := split(_raw, "&");
        for (item :in items) {
            pair := split(item, "=");
            if (pair.count() == 2) {
                _data.set(pair[0], urlDecode(pair[1]));
            }
        }
        return _data.count();
    }

    oper init(String forminput) {
        _raw = forminput;
        _parse();
        _current = true;
    }

    String oper []=(String key, String val) {
        _current = false;
        return _data.set(key, urlDecode(val));
    }

    String get(String key) {
        return _data.get(key);
    }

    String get(String key, String def) {
        return _data.get(key, def);
    }

    String oper[](String key) {
        return _data.get(key);
    }

    String encoded() {
        if (!_current) {
            fmt := StringFormatter();
            bool first = true;
            for (item :in _data) {
                if (!first)
                    fmt.format("&");
                else
                    first = false;
                fmt `$(item.key)=$(urlEncode(item.val))`;
            }
            _raw = fmt.string();
            _current = true;
        }
        return _raw;
    }
}

