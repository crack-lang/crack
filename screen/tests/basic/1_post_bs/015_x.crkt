     uint32 u32 = 100;
     f.format(u32);
     int64 i64 = 123456789;
     i64 = i64 * -123456789;
     f.format(i64);
     uint64 u64 = 123456789;
     u64 = u64 * u64;
     f.format(u64);
     Object o = {};
     f.format(o);
     atomic_int ai;
     ai += 123;
     f.format(ai);

