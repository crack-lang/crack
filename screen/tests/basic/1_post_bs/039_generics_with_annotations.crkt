%%TEST%%
generics with annotations
%%ARGS%%

%%FILE%%
@import crack.ann define;
    import crack.io cout;

     class A[X] {
         @define attr(name) {
             X name;
         }
         @attr(a);
         @attr(b);
     }

     A[int] a = {};
     a.a = 100;
     a.b = 200;

     if (a.a != 100 || a.b != 200)
        cout `FAILED generics with annotations\n`;
     cout `ok\n`;
%%EXPECT%%
ok
%%STDIN%%

