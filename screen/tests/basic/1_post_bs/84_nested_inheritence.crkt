%%TEST%%
tests of issues encountered in nested inheritence
%%ARGS%%
%%FILE%%

import crack.io cout;

if (1) {
    class A { 
        oper init() {} 
        class B : A {
            int f() { return 123; } 
        }
        @static int g() { return B().f(); }
    }
    
    if (A.g() != 123)
        cout `FAILED nested inheritence\n`;
}

if (1) {
    class A {
        oper init() {}
        class B : A {}
        int f() { return 123; }
        @static int g() { return B().f(); }
    }
    if (A.g() != 123)
        cout `FAILED method call in nested inheritence\n`;
}

cout `ok\n`;

%%EXPECT%%
ok
%%STDIN%%
